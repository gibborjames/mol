<h4><%= @partner.name %> Details</h4>

<%= form_for [@partner, @file_upload], :html => { :multipart => true } do |f| %>
	<p>
		<%= f.file_field :file %>
	</p>
	<div class="fileUpload">
		<%= f.submit "Import to Database" %>
	</div>
<% end %>
<table class="table">
	<thead>
		<tr>
			<th>Chassis No</th>
			<th>Size</th>
			<th>Booking No</th>
			<th>Customer</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		<% @items.each do |item| %>
		<tr>
			<td><%= item.chassis_no %></td>
			<td><%= item.size %></td>
			<td><%= item.container_no %></td>
			<td><%= item.customer %></td>
			<td>
				<%= link_to "View", [@partner, item] %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>
